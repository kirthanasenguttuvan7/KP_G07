# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET     /                           controllers.HomeController.index()
GET		/search						controllers.HomeController.getSearchResult(request: play.mvc.Http.Request)

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)

GET		/userProfile/:username		controllers.UserProfileController.UserProfileController.getUserRepos(username:String)
GET		/userProfile				controllers.UserProfileController.UserProfileController.userProfile(username: String, repoArray: String)

GET     /topics/:keyword            controllers.TopicsController.TopicsController.getSearchResult(request: play.mvc.Http.Request, keyword: String)
